{
  "name": "temp-docs",
  "version": "0.0.0",
  "private": true,
  "bin": {
    "lanonasis-mcp": "./mcp-bridge.js"
  },
  "scripts": {
    "docusaurus": "NODE_OPTIONS=\"--require ./scripts/patch-js-yaml.cjs\" docusaurus",
    "start": "NODE_OPTIONS=\"--require ./scripts/patch-js-yaml.cjs\" docusaurus start",
    "prebuild": "bun run check:endpoints || true",
    "build": "NODE_OPTIONS=\"--require ./scripts/patch-js-yaml.cjs\" docusaurus build",
    "swizzle": "NODE_OPTIONS=\"--require ./scripts/patch-js-yaml.cjs\" docusaurus swizzle",
    "deploy": "NODE_OPTIONS=\"--require ./scripts/patch-js-yaml.cjs\" docusaurus deploy",
    "clear": "NODE_OPTIONS=\"--require ./scripts/patch-js-yaml.cjs\" docusaurus clear",
    "serve": "NODE_OPTIONS=\"--require ./scripts/patch-js-yaml.cjs\" docusaurus serve",
    "write-translations": "NODE_OPTIONS=\"--require ./scripts/patch-js-yaml.cjs\" docusaurus write-translations",
    "write-heading-ids": "NODE_OPTIONS=\"--require ./scripts/patch-js-yaml.cjs\" docusaurus write-heading-ids",
    "typecheck": "tsc",
    "index:maas": "ts-node scripts/index-docs-to-maas.ts",
    "index:single": "ts-node scripts/index-docs-to-maas.ts --file",
    "generate:api-docs": "ts-node scripts/generate-api-docs.ts",
    "generate:complete-docs": "ts-node scripts/generate-complete-docs.ts",
    "mcp": "node mcp-bridge.js",
    "check:links": "node scripts/check-links.js",
    "check:endpoints": "node scripts/smoke-endpoints.js",
    "check:all": "bun run check:links && bun run check:endpoints"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.0",
    "@docusaurus/preset-classic": "3.9.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "meilisearch": "^0.53.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "remark": "^15.0.1",
    "strip-markdown": "^6.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.0",
    "@docusaurus/tsconfig": "3.9.0",
    "@docusaurus/types": "3.9.0",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
