{
  "buildCommand": "bun run build",
  "outputDirectory": "build",
  "framework": "docusaurus",
  "installCommand": "bun install",
  "devCommand": "bun run start",
  "functions": {
    "api/**/*": {
      "runtime": "bun"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}